n=[2,5,3,1]
n[3]=7
n.append(9) #adicionar ao fim da
n.append(10)
n.sort() #coloca em ordem
#n.insert(3,0) #adicionar algo a lista mostrando em qual posição
n.insert(1,4)
#n.pop() #elimina o último valor da lista
#n.pop(2) #elimina o número que está na posição 2 da
n.remove(2) #remove o primeiro número 2. Se quiser excluir vários números 2, tem que fazer o laço para percorrer toda a lista
if 12 in n:
    n.remove(12)
print(n)
print(f'Essa lista tem {len(n)} elementos')
