#valores.append(4)
for cont in range(0,5):
    valores.append(int(input(f'Digite o {cont+1}º valor: ')))

for c, v in enumerate(valores): #o enumerate percorre tanto os índices (c), como os valores(v)
    print(f'Na posição {c+1} encontrei o valor {v}')
print('Cheguei ao final da lista')
